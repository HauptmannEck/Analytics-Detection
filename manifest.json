{
  "manifest_version": 2,
  "name": "Detect Analytics DOM",
  "description": "This extension list all the analytics DOM info on the current page.",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Find Analytics DOM"
  },
  "permissions": [
    "activeTab"
  ]
}
